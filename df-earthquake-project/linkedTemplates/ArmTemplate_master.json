{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-earthquake-project"
		},
		"ls_databricks_usgs_data_transformation_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks_usgs_data_transformation'"
		},
		"ls_sink_dl_http_usgs_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sink_dl_http_usgs'"
		},
		"ls_sink_hist_data_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sink_hist_data'"
		},
		"ls_source_hist_data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_source_hist_data'"
		},
		"ls_databricks_usgs_data_transformation_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0919-033905-zhvlnaw7"
		},
		"ls_raw_usgs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://earthquake.usgs.gov/earthquakes/feed/v1.0/summary/"
		},
		"ls_sink_dl_http_usgs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlearthquakeproject.dfs.core.windows.net/"
		},
		"ls_sink_hist_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlearthquakeproject.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-earthquake-project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_databricks_usgs_data_transformation_accessToken": {
						"value": "[parameters('ls_databricks_usgs_data_transformation_accessToken')]"
					},
					"ls_sink_dl_http_usgs_accountKey": {
						"value": "[parameters('ls_sink_dl_http_usgs_accountKey')]"
					},
					"ls_sink_hist_data_accountKey": {
						"value": "[parameters('ls_sink_hist_data_accountKey')]"
					},
					"ls_source_hist_data_connectionString": {
						"value": "[parameters('ls_source_hist_data_connectionString')]"
					},
					"ls_databricks_usgs_data_transformation_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_usgs_data_transformation_properties_typeProperties_existingClusterId')]"
					},
					"ls_raw_usgs_properties_typeProperties_url": {
						"value": "[parameters('ls_raw_usgs_properties_typeProperties_url')]"
					},
					"ls_sink_dl_http_usgs_properties_typeProperties_url": {
						"value": "[parameters('ls_sink_dl_http_usgs_properties_typeProperties_url')]"
					},
					"ls_sink_hist_data_properties_typeProperties_url": {
						"value": "[parameters('ls_sink_hist_data_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}